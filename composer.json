{
    "name": "simettric/sense",
    "description": "The Sense Framework for WordPress",
    "version": "2.0.0",
    "keywords": ["wordpress-plugin"],
    "type":     "wordpress-plugin",
    "homepage": "http://simettric.com/sense",
    "license": "MIT",
    "authors": [
        {
            "name": "Asier Marqus",
            "email": "asier@simettric.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/yaml": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/config": "^3.0",
        "symfony/http-foundation": "^3.0",
        "danielgsims/php-collections": "^1.4"
    },
    "extra": {
        "installer-name": "wordpress-plugin"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": { "Simettric\\Sense\\": "" }
    },
    "minimum-stability": "dev"
}
